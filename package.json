		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"eslint": "^9.33.0",
		"eslint-config-next": "15.5.0",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"postcss": "^8.5.6",
